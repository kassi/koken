{
	"name": "Shortcodes",
	"version": "1.0",
	"description": "Built-in shortcodes",
	"demo": "http://koken.me",
	"author": {
		"name": "Koken",
		"link": "http://koken.me"
	},

	"oembeds": {
		"twitter": {
			"title": "Twitter",
			"endpoint": "https://api.twitter.com/1/statuses/oembed.json",
			"regex": "https?://(www\\.)?twitter.com/.*/status(es)?/\\d+",
			"icon": "data:image/png;base64,"
		},
		"vimeo": {
			"title": "Vimeo",
			"endpoint": "https://vimeo.com/api/oembed.json",
			"regex": "https?://(www\\.)?vimeo.com/.*",
			"icon": "data:image/png;base64,",
			"has_thumbnail": true
		},
		"flickr": {
			"title": "Flickr",
			"endpoint": "https://www.flickr.com/services/oembed/?format=json",
			"regex": "https?://(www\\.)?flickr.com/photos/.*/\\d+(/in/.*)?/?",
			"icon": "data:image/png;base64,",
			"has_thumbnail": true
		},
		"hulu": {
			"title": "Hulu",
			"endpoint": "http://www.hulu.com/api/oembed.json",
			"regex": "https?://(www\\.)?hulu.com/watch/\\d+(/.*)?",
			"icon": "data:image/png;base64,",
			"has_thumbnail": true
		},
		"youtube": {
			"title": "YouTube",
			"endpoint": "https://www.youtube.com/oembed?maxwidth=1920&maxheight=1080&format=json",
			"regex": "https?://(www\\.)?(youtu.be|youtube.com/(watch|playlist)).*",
			"icon": "data:image/png;base64,",
			"has_thumbnail": true
		},
		"instagram": {
			"title": "Instagram",
			"endpoint": "https://api.instagram.com/oembed/",
			"regex": "https?://(www\\.)?(instagr.am|instagram.com)/p/.*",
			"icon": "data:image/png;base64,",
			"has_thumbnail": true
		},
		"soundcloud": {
			"title": "SoundCloud",
			"endpoint": "http://soundcloud.com/oembed?format=json",
			"regex": "https?://(www\\.)?(snd.sc|soundcloud.com)/.*",
			"icon": "data:image/png;base64,"
		}
	},

	"shortcodes": {

		"koken_photo": {

			"icon": "data:image/svg+xml;base64,",
			"media_row_icon": "data:image/svg+xml;base64,",
			"media": "photo",
			"supports_linking": true,

			"data": {

				"caption": {
					"label": "Caption",
					"type": "select",
					"options": [
						{ "label": "Show title and caption", "value": "both" },
						{ "label": "Show title only", "value": "title" },
						{ "label": "Show caption only", "value": "caption" },
						{ "label": "Do not show", "value": "none" }
					]
				}

			},

			"title": "Insert library photo",
			"title_alt": "Edit library photo",
			"description": "Command or Control click to select multiple items",
			"description_alt": "Replace photo, display a caption or assign a hyperlink"

		},

		"koken_video": {

			"icon": "data:image/svg+xml;base64,",
			"media_row_icon": "data:image/svg+xml;base64,",
			"media": "video",
			"supports_linking": false,

			"data": {

				"caption": {
					"label": "Caption",
					"type": "select",
					"options": [
						{ "label": "Show title and caption", "value": "both" },
						{ "label": "Show title only", "value": "title" },
						{ "label": "Show caption only", "value": "caption" },
						{ "label": "Do not show", "value": "none" }
					]
				}

			},

			"title": "Insert video",
			"title_alt": "Edit video",
			"description": "Command or Control click to select multiple items",
			"description_alt": "Replace video, display a caption or assign a hyperlink"

		},

		"koken_slideshow": {

			"icon": "data:image/svg+xml;base64,",
			"media_row_icon": "data:image/svg+xml;base64,",
			"media": "collection",
			"title": "Insert slideshow",
			"title_alt": "Edit slideshow",
			"description": "Display individual photos or an album as a slideshow",
			"description_alt": "Assign new collection, display a caption or edit hyperlinks",

			"data": {

				"caption": {
					"label": "Caption",
					"type": "select",
					"options": [
						{ "label": "Do not show", "value": "none" },
						{ "label": "Show title only", "value": "title" },
						{ "label": "Show caption only", "value": "caption" },
						{ "label": "Show title and caption", "value": "both" }
					]
				},

				"link_to": {
					"label": "Click action",
					"type": "select",
					"options": [
						{ "label": "Use theme setting", "value": "default" },
						{ "label": "Do nothing", "value": "false" },
						{ "label": "Advance slideshow", "value": "advance" },
						{ "label": "Load content page", "value": "content" },
						{ "label": "Load album page", "value": "album" },
						{ "label": "Open lightbox", "value": "lightbox" },
						{ "label": "Toggle playback", "value": "playback" }
					]
				}

			}

		},

		"koken_upload": {

			"icon": "data:image/svg+xml;base64,",
			"media_row_icon": "data:image/svg+xml;base64,",
			"media": "upload",
			"title": "Upload and insert image",
			"title_alt": "Edit uploaded image",
			"description": "Upload and insert an image into this essay/page",
			"description_alt": "Replace image, display a caption or assign a hyperlink",

			"data": {
				"title": {
					"label": "Title",
					"type": "string",
					"placeholder": "Enter an image title"
				},
				"caption": {
					"label": "Caption",
					"type": "string",
					"placeholder": "Enter an image caption"
				},
				"link": {
					"label": "Link",
					"type": "string",
					"placeholder": "http://koken.me",
					"validation": {
						"type": "url",
						"allow_empty": true
					}
				},
				"target": {
					"label": "Link target",
					"type": "select",
					"options": [
						{ "label": "Same window", "value": "none" },
						{ "label": "New window", "value": "_blank" }
					]
				}
			}

		},

		"koken_code": {

			"icon": "data:image/svg+xml;base64,",
			"media_row_icon": "data:image/svg+xml;base64,",
			"title": "Insert custom code",
			"description": "Insert JavaScript or HTML code into this essay/page",

			"data": {
				"code": {
					"type": "code",
					"placeholder": "Insert HTML/JS code here."
				}
			}

		},

		"read_more": {

			"icon": "data:image/svg+xml;base64,",
			"media_row_icon": "data:image/svg+xml;base64,",
			"title": "Insert read more",
			"title_alt": "Edit the read more label",
			"description": "Add a page break",
			"description_alt": "Modify the text shown in the read more link",
			"essays_only": true,

			"data": {
				"label": {
					"label": "Label",
					"type": "string",
					"value": "Read more"
				}
			}
		},

		"koken_contact_form": {
			"icon": "data:image/svg+xml;base64,",
			"media_row_icon": "data:image/svg+xml;base64,",
			"title": "Insert contact form",
			"title_alt": "Edit the contact form",
			"description": "Add a contact form to receive emails from site visitors",
			"description_alt": "Edit contact form fields and labels",
			"pages_only": true,
			"media_row_label": "Contact form",

			"data": {
				"fields": {
					"label": "Fields",
					"type": "fields_array",
					"fields": [
						{
							"key": "label",
							"label": "Label",
							"type": "string"
						},
						{
							"key": "type",
							"label": "Type",
							"type": "select",
							"options": [
								{"label": "Text input", "value": "text"},
								{"label": "Text area", "value": "textarea"},
								{"label": "Email address", "value": "email"},
								{"label": "Phone number", "value": "tel"},
								{"label": "Checkbox", "value": "checkbox"}
							]
						},
						{
							"key": "required",
							"label": "Required",
							"type": "boolean",
							"hidden": {
								"index": 1,
								"equals": "checkbox"
							}
						}
					],
					"value": [
						["Your email address", "email", true],
						["Your message", "textarea", true]
					]
				},

				"recaptcha": {
					"label": "Captcha",
					"type": "recaptcha",
					"fields": [
						{ "key": "enabled" },
						{ "key": "site_key", "placeholder": "Site key" },
						{ "key": "secert_key", "placeholder": "Secret key" }
					],
					"value": [false, "", ""]
				},

				"button_label": {
					"label": "Button label",
					"type": "string",
					"value": "Send"
				},
				"success_message": {
					"label": "Success message",
					"type": "string",
					"value": "Your message has been sent."
				}
			}
		}

	}
}
